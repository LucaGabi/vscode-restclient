{
  "name": "gremlin-client",
  "displayName": "Gremlin Client",
  "description": "Gremlin Client for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "Bit",
  "author": {
    "name": "bit",
    "email": "gabiluca@gmail.com"
  },
  "icon": "images/gremlin.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucaGabi/vscode-restclient/tree/gremlin0",
    "email": "gabiluca@gmail.com"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucaGabi/vscode-restclient/tree/gremlin0"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Linters",
    "Programming Languages"
  ],
  "keywords": [
    "gremlin"
  ],
  "activationEvents": [
    "onLanguage:gremlin"
  ],
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "gremlin",
        "aliases": [
          "gremlin"
        ],
        "extensions": [
          ".gremlin",
          ".groovy"
        ],
        "firstLine": "^<.*>$",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gremlin",
        "scopeName": "source.gremlin",
        "path": "./syntaxes/gremlin.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "gremlin",
        "path": "./snippets/gremlin.json"
      }
    ],
    "commands": [
      {
        "command": "gremlin-client.request",
        "title": "Send Request",
        "category": "Gremlin Client"
      }
    ],
    "menus": {
      
    },
    "keybindings": [
      {
        "command": "gremlin-client.request",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus && editorLangId == 'gremlin'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Gremlin Client",
      "properties": {
        "gremlin-client.host": {
          "type": "string",
          "default": "127.0.0.1",
          "scope": "resource",
          "description": "gremlin host"
        },
        "gremlin-client.port": {
          "type": "string",
          "default": 0,
          "scope": "resource",
          "description": "gremlin port"
        }
      }
    },
    "configurationDefaults": {}
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "watch": "webpack --mode development --watch --info-verbosity verbose",
    "tslint": "tslint --project tsconfig.json"
  },
  "devDependencies": {
    "@types/aws4": "^1.5.1",
    "@types/fs-extra": "^5.0.4",
    "@types/got": "^9.6.8",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.38.0",
    "mocha": "^6.1.4",
    "ts-loader": "^5.3.2",
    "tslint": "^6.1.2",
    "typescript": "^3.7.3",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "adal-node": "^0.1.26",
    "applicationinsights": "^1.0.5",
    "array-map": "0.0.0",
    "array-reduce": "0.0.0",
    "aws4": "^1.9.1",
    "code-highlight-linenums": "^0.2.1",
    "combined-stream": "^1.0.5",
    "dayjs": "^1.8.20",
    "dotenv": "^8.2.0",
    "encodeurl": "^1.0.1",
    "filesize": "^3.3.0",
    "flat": "^5.0.2",
    "foreach": "2.0.4",
    "fs-extra": "^5.0.0",
    "got": "^9.6.0",
    "gremlin": "^3.4.4",
    "highlight.js": "^9.6.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.3",
    "httpsnippet": "^1.20.0",
    "iconv-lite": "^0.4.15",
    "indexof": "0.0.1",
    "isarray": "0.0.1",
    "json-stringify-pretty-compact": "^3.0.0",
    "json3": "3.3.0",
    "jsonc-parser": "^2.0.2",
    "jsonpath-plus": "^0.20.1",
    "mime-types": "^2.1.14",
    "object-keys": "0.5.0",
    "pretty-data": "^0.40.0",
    "tough-cookie": "^3.0.1",
    "tough-cookie-file-store-bugfix": "Huachao/tough-cookie-file-store-bugfix",
    "util": "^0.12.3",
    "uuid": "^3.3.2",
    "vis-network": "^6.4.4",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27",
    "yargs-parser": "^11.1.1"
  }
}
